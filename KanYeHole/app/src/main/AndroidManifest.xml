<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dropout.kanyehole" >
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:hardwareAccelerated="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@android:style/Theme.NoTitleBar" >
         >
        <activity
            android:name=".MenuActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".GameActivity"
            android:configChanges="keyboard|keyboardHidden|orientation|screenSize"
            android:label="Game"
            android:parentActivityName=".MenuActivity"
            android:screenOrientation="portrait" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.dropout.kanyehole.MenuActivity" />
        </activity>
        <activity
            android:name=".TayActivity"
            android:configChanges="keyboard|keyboardHidden|orientation|screenSize"
            android:label="Tay"
            android:parentActivityName=".MenuActivity"
            android:screenOrientation="portrait" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.dropout.kanyehole.MenuActivity" />
        </activity>
        <activity
            android:name=".EndActivity"
            android:configChanges="keyboard|keyboardHidden|orientation|screenSize"
            android:label="End"
            android:parentActivityName=".MenuActivity"
            android:screenOrientation="portrait" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.dropout.kanyehole.MenuActivity" />
        </activity>
        <activity
            android:name=".Customize"
            android:label="title_activity_customize" >
        </activity>
        <activity
            android:name=".Leaderboard"
            android:label="title_activity_leaderboard" >
        </activity>
        <activity
            android:name=".CreditsActivity"
            android:label="@string/title_activity_credits" >
        </activity>
    </application>

</manifest>
